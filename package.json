{
  "name": "todo-graphql",
  "version": "1.0.0",
  "description": "Nest TypeScript Graphql Todo API",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "generate": "ts-node generate-typings",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "up": "docker compose up",
    "down": "docker-compose stop",
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create": "hygen seeds create",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts"
  },
  "dependencies": {
    "@apollo/server": "4.9.3",
    "@graphql-tools/utils": "10.0.5",
    "@nestjs/apollo": "12.0.3",
    "@nestjs/common": "10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.2.6",
    "@nestjs/graphql": "12.0.3",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "10.2.6",
    "@nestjs/typeorm": "^10.0.0",
    "apollo-server-express": "^3.12.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "graphql": "16.8.1",
    "graphql-subscriptions": "2.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.6",
    "@types/bcryptjs": "^2.4.5",
    "@types/express": "4.17.18",
    "@types/node": "20.8.2",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "env-cmd": "^10.1.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.6.4",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-morph": "19.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
